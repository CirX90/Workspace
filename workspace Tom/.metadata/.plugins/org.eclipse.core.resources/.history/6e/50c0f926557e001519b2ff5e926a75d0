package aufgabenblatt1;

import java.util.*;

public class Student {

	public String vorname = "";
	public String nachname = "";
	public int matrikelnummer;
	//public List<Student> Pruefungsleistung;
	private List<Pruefungsleistung> eintraege = new LinkedList<Pruefungsleistung>();
	
	public Student(String vorname, String nachname, int matrikelnmummer) {
		this.nachname = nachname;
		this.vorname = vorname;
		this.matrikelnummer = matrikelnummer;
		
	}
	
	public void eintragHinzufuegen(String name, String nummer) {
	    eintraege.add(new Pruefungsleistung(name, nummer));
	  }

}