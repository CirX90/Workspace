package aufgabenblatt2;

public class Rennauto implements Runnable{
	
	public Rennauto() {
		
		Rennauto wagen1 = new Rennauto();
		Rennauto wagen2 = new Rennauto();
		Rennauto wagen3 = new Rennauto();
		
	}
	
	public void status() {
		System.err.println("Wagen Test");
	}
	
	@Override
	public void run() {
	
		System.err.println(": Wagen Test");
	}
	
	public static void main(String[] args) {
		Thread wagen1 = new Thread(new Rennauto());
		wagen1.start();
	}
	
}