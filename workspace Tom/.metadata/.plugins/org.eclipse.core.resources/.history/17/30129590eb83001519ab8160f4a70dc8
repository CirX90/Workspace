package aufgabenblatt1;

public class ArrayAnwendung {

	public static <T> void main(String[] args) {
		
		int element1 = 5;
		int element2 = 4;
		int element3 = 3;
		
		ArrayList list = new ArrayList<T>(4);
		
		list.hinzufuegen2(element1);
		list.hinzufuegen2(element2);
		list.hinzufuegen2(element1);

		System.out.println(list.toString());
		

	}

}