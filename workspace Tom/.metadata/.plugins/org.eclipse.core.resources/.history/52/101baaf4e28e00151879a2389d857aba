package aufgabenblatt2;

public class Rennen extends Thread{
	
	@Override
	public void run() {
		
		for(int i = 0; i <= 10; i++) {
			System.err.print("Yolo");
		}
		
	}
	
	public static void main(String[] args) {
		Thread rennen = new Thread(new Rennauto());
		rennen.start();
	}

}