package aufgabenblatt1_1;

public class StudentenAnwendung {
	
	public static void main(String[] args) {
		
		System.out.println("---");
	    Student student = new Student();
	    student.eintragHinzufuegen(666666, "Acholl", "Mehmet");
	    student.eintragHinzufuegen(777777, "Aäßler", "Ikke");
	    student.eintragHinzufuegen(111111, "Aidane", "Zizu");
	    student.eintragHinzufuegen(222222, "Aahm", "Philipp");
	    student.eintragHinzufuegen(555555, "Arnold", "Maximilian");
	    student.eintragHinzufuegen(333333, "Aubameyang", "Pierre-Emerick");
	    
	    System.out.println(student);
//	    student.sortieren();
	    System.out.println("Sortiert:");
	    System.out.println(student);
	}

}