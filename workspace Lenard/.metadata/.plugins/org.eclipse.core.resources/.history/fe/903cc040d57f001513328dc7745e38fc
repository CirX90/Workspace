package test;

public class xmldtdtest {
	

			<!ELEMENT Sensoren (ort,typ,wert)*>
			<!ELEMENT Ort (#PCDATA)>			
			<!ELENENT Typ (#PCDATA)>			
			<!ELEMENT Wert EMPTY>				
			<!ATTLIST Wert 22.4 CDATA #REQUIRED>		

			<!ELEMENT Sensoren (ort,wert)*>
			<!ELEMENT Ort
			<!ELEMENT Wert

			<!ELEMENT Sensoren (ort,typ,wert)*>
			<!ELEMENT Ort
			<!ELEMENT Typ
			<!ELEMENT Wert
}
