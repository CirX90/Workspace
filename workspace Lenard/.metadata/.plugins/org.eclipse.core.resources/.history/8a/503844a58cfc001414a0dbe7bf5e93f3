package aufgabenblatt4_2;

public class Wagen {

	public final int laenge;
	public final int passagierkapazitaet;
	
	public Wagen(int laenge, int passagierkapazitaet) {
	
		this.laenge = laenge;
		this.passagierkapazitaet = passagierkapazitaet;
		
		Wagen naechsterWagen = new Wagen(0, 0);
	}
	
	public void setNaechsterWagen() {
		
	}
	
	public int getNaechsterWagen() {
		return naechsterWagen;
	}
}