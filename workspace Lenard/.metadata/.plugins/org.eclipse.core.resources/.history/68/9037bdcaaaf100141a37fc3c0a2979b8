package student;

public class Student {

	// Variablen
	private final int matrikelnummer;
	private String name;
	
	// Konstruktor
	public Student(int matrikelnummer, String name) {
		
		this.matrikelnummer = matrikelnummer;
		this.name = name;
		
	//	this.energie = 10;
	}
	
	// Getter
	public int getMatrikelnummer() {
		return this.matrikelnummer;
	}
	
	// Setter
	public void setName(String name) {
		this.name = name;
	}
	
	// Hauptmethoden
	public void feiern() {
		
	//	this.energie = this.energie - 2;
		
		System.out.println("Student war feiern.");
	}
	
	public void schlafen() {
		
	//	this.energie = this.energie +1;
		System.out.println("Student war schlafen!");
		
	}
	
}
