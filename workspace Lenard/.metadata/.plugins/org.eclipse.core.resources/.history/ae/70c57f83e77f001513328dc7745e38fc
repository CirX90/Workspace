package aufgabenblatt1;

public class StudentenAnwendung {

	public static void main(String[] args) {
		testeEintrag();
	}

	public static void testeEintrag() {
		System.out.println("---");
		Student eintrag = new Student();
		eintrag.eintragHinzufuegen("scholl", "memet", 333333);
		eintrag.eintragHinzufuegen("ikke","asd" ,111111);
		eintrag.eintragHinzufuegen("zizu","asffas", 222222);

		System.out.println(eintrag);
		eintrag.sortieren();
		System.out.println(eintrag);
	}

}
