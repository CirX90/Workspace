package aufgabenblatt4_2;

public class Lokomotive {

	public final int laenge;
	public final int typ;
	
	// Konstruktor erzeugt eine neue Lokomotive
	// erwartet Länge und Typ
	public Lokomotive(int laenge, int typ) {
		this.laenge = laenge;
		this.typ = typ;
		
		Wagen ersterWagen = new Wagen(0, 0);
	}
	
	public int getLaenge() {
		return laenge;
	}
	
	public int getTyp() {
		return typ;
	}
	
	public void setLaenge() {
		
	}
	
	public void setTyp() {
		
	}
}