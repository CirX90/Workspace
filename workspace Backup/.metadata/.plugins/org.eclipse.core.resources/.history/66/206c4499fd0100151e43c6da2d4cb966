package aufgabenblatt4_2;

public class Kontrollstation {

	public static void main(String[] args) {
		
		// Objekt von Zug erstellen und eine Lok hinzugeben
		Lokomotive lok1 = new Lokomotive(40, 1337);
		Zug zug1 = new Zug(lok1);
		Wagen wagen1 = new Wagen(70, 20);
		
		System.out.println(wagen1.toString());
		
		
		zug1.wagenHinzufuegen(new Wagen(70, 20));
	}

}